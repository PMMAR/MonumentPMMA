                        <TextBox x:Name="revnerBox3" TextWrapping="Wrap" Text="3" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,70,0,0"/>
                        <TextBox x:Name="revnerBox4" TextWrapping="Wrap" Text="4" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,105,0,0"/>
                    </Grid>
                </Grid>
                <Grid x:Name="tabteDeleGrid" Margin="0,2575,0,0" Height="260" VerticalAlignment="Top">
                    <TextBlock x:Name="tabteDeleBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Tabte Dele" Foreground="#FF0F5A30"/>
                    <Grid x:Name="tabteDeleBoxGrid" Margin="0,57,0,0" Height="150" VerticalAlignment="Top">
                        <TextBox x:Name="tabteDeleBox1" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18"/>
                        <TextBox x:Name="tabteDeleBox2" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,35,0,0"/>
                        <TextBox x:Name="tabteDeleBox3" TextWrapping="Wrap" Text="3" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,70,0,0"/>
                        <TextBox x:Name="tabteDeleBox4" TextWrapping="Wrap" Text="4" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,105,0,0"/>
                    </Grid>
                </Grid>
                <Grid x:Name="andreSkaderGrid" Margin="0,2845,0,0" Height="260" VerticalAlignment="Top">
                    <TextBlock x:Name="andreSkaderBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Andre Skader" Foreground="#FF0F5A30"/>
                    <Grid x:Name="andreSkaderBoxGrid" Margin="0,57,0,0" Height="150" VerticalAlignment="Top">
                        <TextBox x:Name="andreSkaderBox1" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18"/>
                        <TextBox x:Name="andreSkaderBox2" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,35,0,0"/>
                        <TextBox x:Name="andreSkaderBox3" TextWrapping="Wrap" Text="3" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,70,0,0"/>
                        <TextBox x:Name="andreSkaderBox4" TextWrapping="Wrap" Text="4" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,105,0,0"/>
                    </Grid>
                </Grid>
            </Grid>
        </ScrollViewer>
        <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" Margin="0,591,0,0" VerticalAlignment="Top" Height="54" Width="90"/>
        <Button x:Name="button2" Content="Button" HorizontalAlignment="Left" Margin="90,591,0,-5" VerticalAlignment="Top" Height="54" Width="90"/>
        <Button x:Name="button3" Content="Button" HorizontalAlignment="Left" Margin="180,591,0,0" VerticalAlignment="Top" Height="49" Width="90"/>
        <Button x:Name="button4" Content="Button" HorizontalAlignment="Left" Margin="270,591,0,0" VerticalAlignment="Top" Height="49" Width="90"/>
    </Grid>
</Page>
