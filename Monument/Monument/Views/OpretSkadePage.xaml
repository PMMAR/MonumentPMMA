    xmlns:local="using:Monument.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:Interactivity="using:Microsoft.Xaml.Interactivity" xmlns:Core="using:Microsoft.Xaml.Interactions.Core"
    x:Class="Monument.Views.OpretSkadePage"
    mc:Ignorable="d">

    <Grid Background="#FF333333">
        <ScrollViewer Margin="0,0,0,54" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition">
            <Grid x:Name="ScrollGrid" Height="3726" Width="335" VerticalAlignment="Top">
                <Grid x:Name="photoButtonGrid" Width="200" Height="200" d:LayoutOverrides="VerticalAlignment, Height" VerticalAlignment="Top" Margin="0,30,0,0">
                    <Button x:Name="button" Content="Button" HorizontalAlignment="Stretch" Height="200" Margin="0" VerticalAlignment="Stretch" Width="200" Background="#33FFFFFF"/>

                </Grid>
                <Grid x:Name="typesGrid" Margin="0,250,0,0" Height="195" VerticalAlignment="Top">
                    <TextBlock x:Name="typesBlock" Margin="0" TextWrapping="Wrap" Text="Types" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Foreground="#FF0F5A30"/>
                    <Grid x:Name="CheckBoxes" Margin="0,48,0,0" d:LayoutOverrides="TopPosition, BottomPosition" Height="155" VerticalAlignment="Top">
                        <CheckBox x:Name="skulpturCheckBox" Content="Skulptur" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="40" d:LayoutOverrides="Width, LeftPosition, RightPosition" BorderThickness="0" FontSize="24" Foreground="#FF0F5A30"/>
                        <CheckBox x:Name="sokkelCheckBox" Content="Sokkel" HorizontalAlignment="Stretch" Margin="0,35,0,0" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="24" Foreground="#FF0F5A30"/>
                        <CheckBox x:Name="reliefCheckBox" Content="Relief" HorizontalAlignment="Stretch" Margin="0,70,0,0" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="24" Foreground="#FF0F5A30"/>
                        <CheckBox x:Name="vandkunstCheckBox" Content="Vandkunst" HorizontalAlignment="Stretch" Margin="0,105,0,0" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="24" Foreground="#FF0F5A30"/>
                    </Grid>
                </Grid>
                <Grid x:Name="placeringsGrid" Margin="0,450,0,0" Height="196" VerticalAlignment="Top">
                    <TextBlock x:Name="placeringsBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Foreground="#FF0F5A30">
						<Run Text="Placering"/>
						<LineBreak/>
						<Run/>
                    </TextBlock>
                    <Grid x:Name="CheckBoxes1" Margin="0,57,0,0" Height="133" VerticalAlignment="Top">
                        <CheckBox x:Name="jordCheckBox_Copy" Content="Jord" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="40" BorderThickness="0" FontSize="24" Foreground="#FF0F5A30" d:LayoutOverrides="Width, LeftPosition, RightPosition"/>
                        <CheckBox x:Name="bygningCheckBox_Copy" Content="Bygning" HorizontalAlignment="Stretch" Margin="0,35,0,0" VerticalAlignment="Top" Height="40" FontSize="24" Foreground="#FF0F5A30" d:LayoutOverrides="Width"/>
                        <CheckBox x:Name="facadeCheckBox_Copy" Content="Facade" HorizontalAlignment="Stretch" Margin="0,70,0,0" VerticalAlignment="Top" Height="40" FontSize="24" Foreground="#FF0F5A30" d:LayoutOverrides="Width"/>
                    </Grid>
                </Grid>
                <Grid x:Name="noteGrid" Margin="0,646,0,0" Height="270" VerticalAlignment="Top">
                    <TextBlock x:Name="noteBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Note" Foreground="#FF0F5A30"/>
                    <Grid x:Name="noteBoxGrid" Margin="0,59,0,-36" d:LayoutOverrides="Height" Height="216" VerticalAlignment="Top">
                        <TextBox x:Name="noteBox" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="170" Foreground="White"/>
                        <Button x:Name="saveNoteButton" Content="Gem Note" VerticalAlignment="Bottom" Width="167.5" Height="45">
                            <!--<Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction/>
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>-->
                        </Button>
                    </Grid>
                </Grid>
                <Grid x:Name="biologiskVækstGrid" Margin="0,955,0,0" Height="260" VerticalAlignment="Top">
                    <TextBlock x:Name="bioVækstBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Biologik Vækst" Foreground="#FF0F5A30"/>
                    <Grid x:Name="bioVægstBoxGrid" Margin="0,57,0,0" Height="150" VerticalAlignment="Top">
                        <TextBox x:Name="bioVægstBox1" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18"/>
                        <TextBox x:Name="bioVægstBox2" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,35,0,0"/>
                        <TextBox x:Name="bioVægstBox3" TextWrapping="Wrap" Text="3" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,70,0,0"/>
                        <TextBox x:Name="biovægstBox4" TextWrapping="Wrap" Text="4" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,105,0,0"/>
                    </Grid>
                </Grid>
                <Grid x:Name="forvaltningGrid" Margin="0,1225,0,0" Height="260" VerticalAlignment="Top">
                    <TextBlock x:Name="forvaltningBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Forvaltning" Foreground="#FF0F5A30"/>
                    <Grid x:Name="forvaltningBoxGrid" Margin="0,57,0,0" Height="150" VerticalAlignment="Top">
                        <TextBox x:Name="forvaltningBox1" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18"/>
                        <TextBox x:Name="forvaltningBox2" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,35,0,0"/>
                        <TextBox x:Name="forvaltningBox3" TextWrapping="Wrap" Text="3" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,70,0,0"/>
                        <TextBox x:Name="forvaltningBox4" TextWrapping="Wrap" Text="4" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,105,0,0"/>
                    </Grid>
                </Grid>
                <Grid x:Name="konstruktionsSkaderGrid" Margin="0,1495,0,0" Height="260" VerticalAlignment="Top">
                    <TextBlock x:Name="konstruktionsSkaderBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Konstruktions Skader" Foreground="#FF0F5A30"/>
                    <Grid x:Name="konstruktionsSkaderBoxGrid" Margin="0,57,0,0" Height="150" VerticalAlignment="Top">
                        <TextBox x:Name="konstSkaBox1" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18"/>
                        <TextBox x:Name="konstSkaBox2" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,35,0,0"/>
                        <TextBox x:Name="konstSkaBox3" TextWrapping="Wrap" Text="3" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,70,0,0"/>
                        <TextBox x:Name="konstSkaBox4" TextWrapping="Wrap" Text="4" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,105,0,0"/>
                    </Grid>
                </Grid>
                <Grid x:Name="mangelfuldeFuger" Margin="0,1765,0,0" Height="260" VerticalAlignment="Top">
                    <TextBlock x:Name="mangelfuldeFugerBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Mangelfulde Fuger" Foreground="#FF0F5A30"/>
