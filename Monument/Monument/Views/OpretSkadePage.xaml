                        <TextBox x:Name="mangFuBox1" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18"/>
                        <TextBox x:Name="mangFuBox2" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,35,0,0"/>
                        <TextBox x:Name="mangFuBox3" TextWrapping="Wrap" Text="3" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,70,0,0"/>
                        <TextBox x:Name="mangFuBox4" TextWrapping="Wrap" Text="4" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,105,0,0"/>
                    </Grid>
                </Grid>
                <Grid x:Name="misfarvning" Margin="0,2035,0,0" Height="260" VerticalAlignment="Top">
                    <TextBlock x:Name="misfarvningBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Misfarvning" Foreground="#FF0F5A30"/>
                    <Grid x:Name="misfarvningBoxGrid" Margin="0,57,0,0" Height="150" VerticalAlignment="Top">
                        <TextBox x:Name="misfarvningBox1" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18"/>
                        <TextBox x:Name="misfarvningBox2" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,35,0,0"/>
                        <TextBox x:Name="misfarvningBox3" TextWrapping="Wrap" Text="3" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,70,0,0"/>
                        <TextBox x:Name="misfarvningBox4" TextWrapping="Wrap" Text="4" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18" Margin="0,105,0,0"/>
                    </Grid>
                </Grid>
                <Grid x:Name="revner" Margin="0,2305,0,0" Height="260" VerticalAlignment="Top">
                    <TextBlock x:Name="revnerBlock" Margin="0" TextWrapping="Wrap" d:LayoutOverrides="Width" Height="52" VerticalAlignment="Top" FontSize="32" Text="Revner" Foreground="#FF0F5A30"/>
                    <Grid x:Name="revnerBoxGrid" Margin="0,57,0,0" Height="150" VerticalAlignment="Top">
                        <TextBox x:Name="revnerBox1" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" d:LayoutOverrides="Width" Height="40" FontSize="18"/>
